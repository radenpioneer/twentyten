{
  "name": "twenty-ten",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "conc npm:check && conc npm:gen:icons && astro build",
    "cf-typegen": "wrangler types",
    "check": "conc npm:check:*",
    "check:astro": "astro check",
    "check:format": "prettier --check .",
    "deploy": "conc npm:build && wrangler pages deploy",
    "dev": "KEYSTATIC=true astro dev",
    "format": "prettier --write .",
    "gen:icons": "pwa-assets-generator --preset minimal-2023 public/favicon.svg",
    "preview": "conc npm:build && wrangler pages dev",
    "start": "conc npm:dev"
  },
  "dependencies": {
    "@astrojs/cloudflare": "11.0.4",
    "@astrojs/react": "3.6.2",
    "@astrojs/rss": "4.0.7",
    "@astrojs/sitemap": "3.1.6",
    "@astrojs/tailwind": "5.1.0",
    "@keystatic/astro": "5.0.0",
    "@keystatic/core": "0.5.32",
    "astro": "4.14.2",
    "fuse.js": "7.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.3",
    "@cloudflare/workers-types": "4.20240815.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.14",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vite-pwa/assets-generator": "0.2.4",
    "concurrently": "8.2.2",
    "marked": "14.0.0",
    "prettier": "3.3.3",
    "prettier-config-standard": "7.0.0",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "wrangler": "3.72.0"
  }
}
